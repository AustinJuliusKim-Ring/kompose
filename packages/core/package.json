{
  "name": "@kompose/core",
  "version": "0.0.1",
  "description": "Plain ol' function composition tools",
  "main": "build/index.js",
  "scripts": {
    "build:cjs": "babel src --out-dir build --extensions \".ts,.tsx\"",
    "build:ts": "tsc --emitDeclarationOnly",
    "build": "npm run build:ts && npm run build:cjs",
    "clean": "rimraf build esm coverage",
    "coverage": "jest --coverage",
    "dev": "concurrently \"npm:types:watch\" \"npm:test:watch\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "types": "tsc --noEmit",
    "types:watch": "tsc --noEmit --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrarnoldpalmer/kompose.git"
  },
  "keywords": [
    "[\"js\"",
    "\"javascript\"",
    "\"functional",
    "programming\"",
    "\"ADTs\"",
    "\"composition\"]"
  ],
  "author": "Mitchell Valine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrarnoldpalmer/kompose/issues"
  },
  "homepage": "https://github.com/mrarnoldpalmer/kompose#readme"
}
